{
	"coc.preferences.formatOnType": false,
	"coc.preferences.rootPatterns": [
		".vim",
		".git",
		".hg",
		"composer.json",
		"bower.json",
		"packages.json",
		".projections.json"
	],
	"suggest.autoTrigger": "always",
	"suggest.noselect": true,
	"suggest.enablePreselect": true,
	"suggest.completionItemKindLabels": {
		"keyword": "",
		"variable": "",
		"value": "",
		"operator": "Ψ",
		"constructor": "",
		"function": "ƒ",
		"reference": "渚",
		"constant": "",
		"method": "",
		"struct": "פּ",
		"class": "",
		"interface": "",
		"text": "",
		"enum": "",
		"enumMember": "",
		"module": "",
		"color": "",
		"property": "",
		"field": "料",
		"unit": "",
		"event": "鬒",
		"file": "",
		"folder": "",
		"snippet": "",
		"typeParameter": "",
		"default": ""
	},
	"session.directory": "/Users/imxieke/.local/share/nvim/sessions",
	"session.restartOnSessionLoad": true,
	"snippets.priority": 90,
	"snippets.loadFromExtensions": true,
	"snippets.loadVSCodeProjectSnippets": true,
	"snippets.textmateSnippetsRoots": [ // 支持在此定义 VSCode Snippets 目录
		"Library/Application Support/Code/User/snippets"
	],
	"snippets.ultisnips.directories": [
		"UltiSnips",
		"gosnippets/UltiSnips"
	],
	"snippets.extends": {
		"cpp": [
			"c"
		],
		"Dockerfile*": [
			"dockerfile"
		],
		"javascriptreact": [
			"javascript"
		],
		"typescript": [
			"javascript"
		]
	},
	"languageserver": {
		"go": {
			"command": "gopls",
			"rootPatterns": [
				"go.mod"
			],
			"trace.server": "verbose",
			"filetypes": [
				"go"
			]
		},
		"nginx-language-server": {
			"command": "nginx-language-server",
			"filetypes": [
				"nginx"
			],
			"rootPatterns": [
				"nginx.conf"
			]
		}
	},
	"colors.filetypes": [
		"*"
	],
	"highlight.colors.enable": true,
	"highlight.colorNames.enable": true,
	"highlight.document.enable": true,
	"highlight.disableLanguages": [],
	"html-css-support.enable": true,
	"html-css-support.enabledLanguages": [
		"html",
		"vue",
		"blade",
		"htmldjango",
		"typescriptreact",
		"javascriptreact"
	],
	"html-css-support.styleSheets": [
		"https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css",
		"/style.css",
		"style.css"
	],
	"json.enable": true,
	"json.format.enable": true,
	"json.schemas": [
		{
			"fileMatch": [
				"*.json"
			]
		}
	],
	"pyright.enable": true,
	"pyright.server": "/usr/local/bin/pyright-langserver",
	"python.analysis.autoImportCompletions": true,
	"python.analysis.autoSearchPaths": true,
	"python.analysis.diagnosticMode": "openFilesOnly",
	"python.pythonPath": "/usr/local/bin/python3",
	"python.linting.flake8Enabled": true,
	"python.linting.banditEnabled": true,
	"python.linting.mypyEnabled": true,
	"python.linting.pytypeEnabled": true,
	"python.linting.pycodestyleEnabled": true,
	"python.linting.prospectorEnabled": true,
	"python.linting.pydocstyleEnabled": true,
	"python.linting.pylamaEnabled": true,
	"python.linting.pylintEnabled": true,
	"pylsp.trace.server": "Off",
	"pylsp.commandPath": "/usr/local/bin/pylsp",
	"pylsp.connectionMode": "stdio",
	"pylsp.builtin.pythonPath": "/usr/local/bin/python3",
	"pylsp.builtin.enableInstallPylspMypy": true,
	"pylsp.builtin.enableInstallPylsIsort": true,
	"pylsp.builtin.enableInstallPythonLspBlack": true,
	"javascript.preferences.quoteStyle": "double",
	"tsserver.maxTsServerMemory": "1024M",
	"typescript.preferences.quoteStyle": "double",
	"go.goplsPath": "/Users/imxieke/.go/bin/gopls",
	"intelephense.enable": true,
	"intelephense.path": "/usr/local/bin/intelephense",
	"intelephense.pest.path": "/Users/imxieke/.composer/vendor/bin/pest",
	"intelephense.composer.path": "/Users/imxieke/.bin/composer",
	"intelephense.phpunit.colors": true,
	"intelephense.phpunit.debug": true,
	"intelephense.completion.fullyQualifyGlobalConstantsAndFunctions": true,
	"intelephense.files.maxSize": 5000000,
	"intelephense.maxMemory": 1024,
	"intelephense.trace.server": "messages",
	"intelephense.references.exclude": [
		"**/vendor/**"
	],
	"intelephense.rename.exclude": [
		"**/vendor/**"
	],
	"intelephense.files.associations": [
		"**/.git/**",
		"**/.svn/**",
		"**/.hg/**",
		"**/CVS/**",
		"**/.DS_Store/**",
		"**/node_modules/**",
		"**/bower_components/**",
		"**/vendor/**/{Tests,tests}/**",
		"**/.history/**",
		"**/vendor/**/vendor/**"
	],
	"intelephense.files.exclude": [
		"*.php",
		"*.phtml"
	],
	"codeLens.enable": true,
	"intelephense.client.codelensProvider": "phpunit",
	"intelephense.client.disableInlayHints": true,
	"lightbulb.enableSign": true,
	"webview.debug": true,
	"webview.colorStrategy": true,
	"webview.primaryColors": "dark",
	"markdown-preview-enhanced.enableScriptExecution": true,
	"translator.enableHover": true,
	"phpstan.enable": true,
	"phpstan.toolPath": "/Users/imxieke/.bin/phpstan",
	"phpstan.level": "max",
	"phpstan.memoryLimit": "1024M",
	"phpstan.download.checkOnStartup": false,
	"Lua.telemetry.enable": true,
	"rust-client.enable": true,
	"rust-client.rustupPath": "/Users/imxieke/.cargo/bin/rustup",
	"rust-client.rlsPath": "/Users/imxieke/.cargo/bin/rls",
	"rust-client.channel": "stable",
	"psalm.enable": true,
	"psalm.disableCompletion": false,
	"psalm.disableDefinition": false,
	"psalm.enableDebugLog": true,
	"psalm.unusedVariableDetection": true,
	"psalm.configPaths": [
		"psalm.xml",
		"psalm.xml.dist"
	],
	"psalm.phpExecutablePath": "/usr/local/opt/php@8.0/bin/php",
	"psalm.psalmScriptPath": "/Users/imxieke/.local/share/nvim/language-server-protocol/bin/psalm-language-server",
	"psalm.psalmScriptExtraArgs": [],
	"psalm.phpExecutableArgs": [
		"-dxdebug.remote_autostart=0",
		"-dxdebug.remote_enable=0",
		"-dxdebug_profiler_enable=0"
	],
	"psalm.analyzedFileExtensions": [
		{
			"scheme": "file",
			"language": "php"
		},
		{
			"scheme": "untitled",
			"language": "php"
		}
	],
	"php-cs-fixer.enable": false,
	"php-cs-fixer.toolPath": "/Users/imxieke/.bin/php-cs-fixer",
	"php-cs-fixer.activateTool": "php-cs-fixer",
	"php-cs-fixer.rules": "@PSR12",
	"htmlhint.enable": true,
	"htmlhint.documentSelector": [
		"html",
		"htm",
		"html5",
		"twig",
		"blade",
		"jst"
	],
	"htmlhint.options": {},
	"nginx.enable": true,
	"nginx.server.commandPath": "/usr/local/bin/nginx-language-server",
	"nginx.builtin.pythonPath": "/usr/local/bin/python3",
	"nginx.nginxfmt.commandPath": "/usr/local/bin/nginxfmt",
	"nginx.nginxfmt.indent": 4,
	"volar.enable": true,
	"volar.scaffoldSnippets.enable": true,
	"volar.autoClosingTags": true,
	"volar.autoCompleteRefs": true,
	"volar.diagnostics.enable": true,
	"volar.diagnostics.tsLocale": "zh-cn",
	"volar-tools.enable": true,
	"volar.preview.port": 3333
}